    "description": "Inserts your prettier and tsconfig settings into your projects.",
    "readme": "/README.md",
    "version": "1.0.1",
    "main": "/index.js",
    "license": "MIT",
    "bin": "/index.js",
    "dependencies": {
        "argparse": "^2.0.1"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 80,
        "semi": true,
        "quoteProps": "consistent",
        "bracketSpacing": true,
        "bracketSameLine": true,
        "singleQuote": true,
